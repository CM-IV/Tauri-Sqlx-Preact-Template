{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "deb",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "oceanus-app-tauri",
  "mainBinaryName": "oceanus-app-tauri",
  "version": "0.1.0",
  "identifier": "com.cmiv.oceanus",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Oceanus App",
        "width": 1100,
        "height": 650,
        "minHeight": 600,
        "minWidth": 1100,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}